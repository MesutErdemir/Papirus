{% extends 'cactus/_layout/index.html.twig' %}

{% block title %}{{ page.getTitle() }}{% endblock %}

{% block page_content %}
    <article class="post" itemscope itemtype="http://schema.org/BlogPosting">
        <div class="content" itemprop="articleBody">
            {{ page.getContent()|raw }}
        </div>
    </article>
{% endblock %}
